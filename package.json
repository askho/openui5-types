{
    "version": "0.4.0",
    "name": "openui5-types",
    "description": "A simple adapter to develop SAPUI5 and OpenUI5 applications using TypeScript and ES2015 modules/classes",
    "license": "MIT",
    "keywords": [
        "ui5",
        "openui5",
        "sapui5",
        "typescript",
        "types"
    ],
    "homepage": "https://github.com/r-murphy/openui5-types",
    "author": "Leonardo Machado Carreiro <leonardo.mcarreiro@gmail.com>, Ryan Murphy <rdmurphy@gmail.com>",
    "repository": {
        "type": "git",
        "url": "https://github.com/r-murphy/openui5-types"
    },
    "bugs": {
        "url": "https://github.com/r-murphy/openui5-types/issues"
    },
    "devDependencies": {
        "@types/fs-extra": "^5.0.0",
        "@types/json5": "0.0.29",
        "@types/lodash.includes": "^4.3.3",
        "@types/node": "^9.4.6",
        "@types/request": "^2.0.4",
        "array-includes": "^3.0.3",
        "fs-extra": "^5.0.0",
        "json5": "^0.5.1",
        "lodash.includes": "^4.3.0",
        "request": "^2.83.0",
        "typescript": "^2.7.2"
    },
    "scripts": {
        "prepack": "npm run generate && npm run build:lib",
        "start": "npm run generate && npm run build:lib",
        "generate": "npm run build:generator && node src/generator/index.js",
        "build:gen": "tsc -p src/generator/tsconfig.json",
        "build:gen:watch": "tsc -p src/generator/tsconfig.json -w",
        "build:lib": "tsc -p src/lib/tsconfig.json",
        "build:lib:watch": "tsc -p src/lib/tsconfig.json -w",
        "run:gen": "node src/generator/index.js",
        "run:gen:inspect": "node --inspect src/generator/index.js"
    }
}
