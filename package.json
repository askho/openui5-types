{
    "version": "0.4.1",
    "name": "openui5-types",
    "description": "OpenUI5 TypeScript definitions, exports and (optional) runtime annotations.",
    "license": "MIT",
    "keywords": [
        "ui5",
        "openui5",
        "sapui5",
        "typescript",
        "types"
    ],
    "homepage": "https://github.com/r-murphy/openui5-types",
    "author": "Leonardo Machado Carreiro <leonardo.mcarreiro@gmail.com>, Ryan Murphy <rdmurphy@gmail.com>",
    "repository": {
        "type": "git",
        "url": "https://github.com/r-murphy/openui5-types"
    },
    "bugs": {
        "url": "https://github.com/r-murphy/openui5-types/issues"
    },
    "devDependencies": {
        "@types/fs-extra": "^5.0.0",
        "@types/jquery": "^3.3.1",
        "@types/json5": "^0.0.29",
        "@types/lodash.includes": "^4.3.3",
        "@types/node": "^9.4.7",
        "@types/request": "^2.0.4",
        "array-includes": "^3.0.3",
        "fs-extra": "^5.0.0",
        "json5": "^1.0.0",
        "lodash.includes": "^4.3.0",
        "request": "^2.85.0",
        "typescript": "^2.7.2"
    },
    "scripts": {
        "prepack": "npm run generate",
        "start": "npm run generate",
        "generate": "npm run build:gen && node src/generator/index.js",
        "build:gen": "tsc -p src/generator/tsconfig.json",
        "build:gen:watch": "tsc -p src/generator/tsconfig.json -w",
        "build:ann": "tsc -p src/annotations/tsconfig.json",
        "build:ann:watch": "tsc -p src/annotations/tsconfig.json -w",
        "run:gen": "node src/generator/index.js",
        "run:gen:inspect": "node --inspect src/generator/index.js"
    }
}
