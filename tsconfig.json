{
    "compilerOptions": {
        "target": "es2015",
        "module": "none",
        "moduleResolution": "node",
        "noUnusedLocals": true,
        "experimentalDecorators": true,
        "alwaysStrict": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "sourceMap": true
    },
    "exclude": [
        "./types"
    ]
}
